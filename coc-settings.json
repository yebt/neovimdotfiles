{
    "emmet.includeLanguages": {"vue-html": "html", "javascript": "javascriptreact", "php":"html", "blade":"html"},

    "markdownlint.config": {
        "line_length": false
    },

    "diagnostic.errorSign": "x",
    "diagnostic.infoSign": "!",
    //"diagnostic.warningSign": "⚠",
    "diagnostic.hintSign": "¿",

    "explorer.icon.enableNerdfont": true,
    "explorer.icon.source": "nvim-web-devicons",
    "explorer.file.showHiddenFiles": true,
    "explorer.file.column.indent.indentLine": true,
    "explorer.enableFloatinput": true,

     "suggest.noselect": false,
    "suggest.floatEnable": false
}
